#include <stdio.h>
#include<iostream>
using namespace std;

class wizard
{
    int a;
    int b;
    public:
    void print()
    {
        printf(" %d\n",a);
    }
    
    wizard(int a=30,int b=90)
    {
        this->a=a;
        this->b=b;
        cout<<"a "<<a<<"  b  "<<b<<endl;
    
    }
    wizard(wizard &s)
    {
        a=s.a;
        b=s.b;
    }
   ~wizard()
   {
       
       cout<<" bye bye "<<a<<" "<<b;
   }
};


int main()
{
    wizard t2(2,4);
    wizard t3(t2);
    
}
