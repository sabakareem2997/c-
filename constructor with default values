#include <stdio.h>
#include<iostream>
using namespace std;

class wizard
{
    int a;
    public:
    void print()
    {
        printf(" %d\n",a);
    }
    
    wizard(int a=30,int b=90)
    {
        cout<<"a "<<a<<"  b  "<<b<<endl;
    
    }
   
};


int main()
{
    
    wizard t;
    wizard t2(2,4);
    wizard t3(2);
    
}
