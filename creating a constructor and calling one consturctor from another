#include <stdio.h>
#include<iostream>
using namespace std;

class wizard
{
    int a;
    public:
    void print()
    {
        printf(" %d\n",a);
    }
    wizard()
    {
        a=0;
    printf("default constructor\n");
    }
    
    wizard(int a,int b)
    {
        a=10;
        printf("parameterized constructor\n");
    }
    wizard(int a)
    {
        
        printf("one toe\n");
        wizard();
    }
};


int main()
{
    
    wizard t;
    wizard t2(2,4);
    wizard t3(2);
    
}
