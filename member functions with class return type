class kareem
{
    int a;
    int b;
    public:
    void get_data();
    kareem big(kareem a);
    void print()
    {
        printf("%d",a);
    }
};

void kareem::get_data()
{
    cout<<"enter the data";
    cin>>a;
   // cin>>b;
    cout<<"entered data is  "<<a<<endl;
    
}



kareem kareem::big(kareem b)
{
    kareem y;
    if(a>(b.a))
    y.a=b.a;
    else
    y.a=b.a;
    return y;
    
    
}

int main()
{
    kareem t,t1;
    t.get_data();
    t1.get_data();
    kareem t3;
    t3=t.big(t1);
    t3.print();
    
